{
  "name": "@amplication/server",
  "version": "0.10.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:save": "prisma migrate dev",
    "migrate:up": "prisma migrate deploy",
    "prisma:studio": "prisma studio ",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "db:clean": "ts-node prisma/clean.ts",
    "docker": "docker-compose up -d",
    "docker:down": "docker-compose down --volumes",
    "generate-graphql-schema": "ts-node -r tsconfig-paths/register scripts/generate-graphql-schema",
    "check-graphql-schema": "ts-node -r tsconfig-paths/register scripts/check-graphql-schema"
  },
  "dependencies": {
    "@amplication/container-builder": "^0.10.1",
    "@amplication/data": "^0.10.1",
    "@amplication/data-service-generator": "^0.10.1",
    "@amplication/deployer": "^0.10.1",
    "@codebrew/nestjs-storage": "0.1.4",
    "@extra-set/difference": "2.2.3",
    "@google-cloud/cloudbuild": "2.0.3",
    "@google-cloud/secret-manager": "3.8.1",
    "@google-cloud/trace-agent": "5.1.3",
    "@nestjs/common": "7.6.18",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.18",
    "@nestjs/graphql": "7.5.5",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.6",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/serve-static": "2.2.2",
    "@octokit/core": "3.5.1",
    "@octokit/oauth-app": "2.0.3",
    "@octokit/openapi-types": "2.3.1",
    "@octokit/rest": "18.6.7",
    "@prisma/client": "2.19.0",
    "@slynova/flydrive": "1.0.3",
    "@slynova/flydrive-gcs": "1.0.3",
    "@types/adm-zip": "0.4.34",
    "@types/bcrypt": "3.0.1",
    "@types/dockerode": "2.5.34",
    "@types/express": "4.17.2",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "25.2.3",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.head": "4.0.6",
    "@types/lodash.last": "3.0.6",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.set": "4.3.6",
    "@types/node": "13.7.4",
    "@types/passport": "1.0.7",
    "@types/passport-github": "1.1.6",
    "@types/passport-jwt": "3.0.6",
    "@types/tar-stream": "2.2.1",
    "@types/triple-beam": "1.3.2",
    "adm-zip": "0.5.5",
    "ajv": "6.12.6",
    "apollo-server-express": "2.15.1",
    "bcrypt": "5.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cuid": "2.1.8",
    "date-fns": "2.22.1",
    "dockerode": "3.2.1",
    "get-stream": "6.0.1",
    "graphql": "14.6.0",
    "graphql-type-json": "0.3.2",
    "lodash.get": "4.4.2",
    "lodash.head": "4.0.1",
    "lodash.last": "3.0.0",
    "lodash.omit": "4.5.0",
    "lodash.set": "4.3.2",
    "nest-morgan": "1.0.1",
    "nest-winston": "1.5.0",
    "nestjs-prisma": "0.12.0",
    "octokit-plugin-create-pull-request": "3.9.3",
    "passport": "0.4.1",
    "passport-github2": "0.1.12",
    "passport-jwt": "4.0.0",
    "pluralize": "8.0.0",
    "prisma": "2.19.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "semver": "7.3.5",
    "supertest": "4.0.2",
    "tar-stream": "2.2.0",
    "type-fest": "0.15.1",
    "typescript": "4.2.3",
    "validate-color": "2.1.1",
    "winston": "3.3.3",
    "winston-cloud-logging": "1.0.2",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/testing": "7.6.18",
    "@types/pluralize": "0.0.29",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "apollo-server-testing": "2.16.1",
    "eslint": "7.23.0",
    "eslint-import-resolver-lerna": "2.0.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-loader": "6.2.2",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.10.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    }
  }
}
