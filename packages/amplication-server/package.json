{
  "name": "amplication-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "migrate:down": "prisma migrate down --experimental",
    "prisma:studio": "prisma studio --experimental",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:remove-db": "docker-compose -f docker-compose.db.yml down --volumes",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "generate-graphql-schema": "ts-node -r tsconfig-paths/register scripts/generate-graphql-schema",
    "check-graphql-schema": "ts-node -r tsconfig-paths/register scripts/check-graphql-schema"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "^0.1.4",
    "@extra-set/difference": "^2.1.52",
    "@google-cloud/secret-manager": "^3.1.0",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/graphql": "^7.5.5",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/serve-static": "^2.1.3",
    "@octokit/rest": "^18.0.6",
    "@prisma/cli": "^2.6.2",
    "@prisma/client": "^2.6.2",
    "@slynova/flydrive": "^1.0.2",
    "@slynova/flydrive-gcs": "^1.0.2",
    "@types/adm-zip": "^0.4.33",
    "@types/bcrypt": "3.0.0",
    "@types/bull": "^3.14.0",
    "@types/express": "4.17.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "25.1.3",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.head": "^4.0.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "13.7.4",
    "@types/passport": "^1.0.3",
    "@types/passport-github": "^1.1.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/triple-beam": "^1.3.2",
    "adm-zip": "^0.4.16",
    "ajv": "^6.12.2",
    "amplication-data": "^0.1.0",
    "amplication-data-service-generator": "^0.1.0",
    "apollo-server-express": "^2.15.1",
    "axios": "^0.20.0",
    "bcrypt": "4.0.0",
    "bull": "^3.16.0",
    "class-transformer": "0.3.1",
    "class-validator": "^0.12.2",
    "graphql": "14.6.0",
    "graphql-type-json": "^0.3.2",
    "lodash.get": "^4.4.2",
    "lodash.head": "^4.0.1",
    "lodash.last": "^3.0.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "nest-morgan": "^1.0.1",
    "nest-winston": "^1.3.6",
    "nestjs-prisma": "^0.6.0-dev.2",
    "passport": "0.4.1",
    "passport-github": "^1.1.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "semver": "^7.3.2",
    "supertest": "^4.0.2",
    "type-fest": "^0.15.1",
    "typescript": "^3.9.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.3.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "apollo-server-testing": "^2.16.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "^1.0.0-rc.3",
    "typegraphql-prisma": "^0.2.3",
    "typegraphql-prisma-nestjs": "^0.100.701"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    }
  }
}
