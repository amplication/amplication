{
  "name": "@amplication/server",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:save": "prisma migrate dev",
    "migrate:up": "prisma migrate deploy",
    "prisma:studio": "prisma studio ",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "db:clean": "ts-node prisma/clean.ts",
    "docker": "docker-compose up -d",
    "docker:down": "docker-compose down --volumes",
    "generate-graphql-schema": "ts-node -r tsconfig-paths/register scripts/generate-graphql-schema",
    "check-graphql-schema": "ts-node -r tsconfig-paths/register scripts/check-graphql-schema"
  },
  "dependencies": {
    "@amplication/container-builder": "^0.6.0",
    "@amplication/data": "^0.6.0",
    "@amplication/data-service-generator": "^0.6.2",
    "@amplication/deployer": "^0.6.0",
    "@codebrew/nestjs-storage": "^0.1.4",
    "@extra-set/difference": "^2.1.52",
    "@google-cloud/cloudbuild": "^2.0.3",
    "@google-cloud/secret-manager": "^3.1.0",
    "@google-cloud/trace-agent": "^5.1.1",
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/graphql": "^7.5.5",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/serve-static": "^2.1.3",
    "@octokit/core": "^3.2.4",
    "@octokit/oauth-app": "^2.0.3",
    "@octokit/rest": "^18.0.6",
    "@prisma/client": "^2.19.0",
    "@slynova/flydrive": "^1.0.2",
    "@slynova/flydrive-gcs": "^1.0.2",
    "@types/adm-zip": "^0.4.33",
    "@types/bcrypt": "3.0.0",
    "@types/dockerode": "^2.5.34",
    "@types/express": "4.17.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "25.1.3",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.head": "^4.0.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "13.7.4",
    "@types/passport": "^1.0.3",
    "@types/passport-github": "^1.1.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/tar-stream": "^2.1.0",
    "@types/triple-beam": "^1.3.2",
    "adm-zip": "^0.4.16",
    "ajv": "^6.12.2",
    "apollo-server-express": "^2.15.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "^0.12.2",
    "cuid": "^2.1.8",
    "date-fns": "^2.16.1",
    "dockerode": "^3.2.1",
    "get-stream": "^6.0.0",
    "graphql": "14.6.0",
    "graphql-type-json": "^0.3.2",
    "lodash.get": "^4.4.2",
    "lodash.head": "^4.0.1",
    "lodash.last": "^3.0.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "nest-morgan": "^1.0.1",
    "nest-winston": "^1.3.6",
    "nestjs-prisma": "^0.6.0-dev.2",
    "octokit-plugin-create-pull-request": "^3.9.1",
    "passport": "0.4.1",
    "passport-github2": "^0.1.12",
    "passport-jwt": "4.0.0",
    "pluralize": "^8.0.0",
    "prisma": "^2.19.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "semver": "^7.3.2",
    "supertest": "^4.0.2",
    "tar-stream": "^2.1.4",
    "type-fest": "^0.15.1",
    "typescript": "^4.2.3",
    "validate-color": "^2.1.0",
    "winston": "^3.2.1",
    "winston-cloud-logging": "^1.0.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/testing": "^7.3.2",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "apollo-server-testing": "^2.16.1",
    "eslint": "^7.23.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "^1.0.0-rc.3",
    "typegraphql-prisma": "^0.2.3",
    "typegraphql-prisma-nestjs": "^0.100.701"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    }
  }
}
