{
  "name": "@amplication/nest-logger-module",
  "version": "0.16.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist",
    "test": "jest",
    "check-format": "prettier --check \"**/*.{ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{ts,json,gql,md,yaml}\""
  },
  "author": "tupe12334",
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@types/node": "18.6.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".spec.ts$",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@nestjs/common": "8.4.3",
    "nest-winston": "1.7.0",
    "reflect-metadata": "0.1.13",
    "winston": "3.8.1",
    "winston-cloud-logging": "1.0.2"
  }
}
