    const permission = this.rolesBuilder.permission({
      role: userRoles,
      action: \\"read\\",
      possession: \\"own\\",
      resource: \\"Order\\",
    });
    const result = await this.service.findOne({
      where: params,
      select: {
        id: true,
        createdAt: true,
        updatedAt: true,

        customer: {
          select: {
            id: true,
          },
        },

        status: true,
        label: true,
