{
  "name": "server-template",
  "version": "0.16.1",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "prisma migrate reset",
    "db:init": "run-s \"db:migrate-save -- --name 'initial version'\" db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "package:container": "docker build .",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down --volumes"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@nestjs/apollo": "12.2.2",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/graphql": "12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "8.1.1",
    "@prisma/client": "^6.2.1",
    "@types/bcrypt": "5.0.2",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.7",
    "graphql-type-json": "0.3.2",
    "nest-access-control": "^3.1.0",
    "graphql": "^16.8.1",
    "npm-run-all": "4.1.5",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "ts-node": "10.9.2",
    "type-fest": "2.19.0",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.19",
    "@types/graphql-type-json": "0.3.5",
    "@types/jest": "^29.5.5",
    "@types/normalize-path": "3.0.2",
    "@types/passport-http": "0.3.11",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^6.2.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@app/custom-validators": "<rootDir>/src/validators"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
