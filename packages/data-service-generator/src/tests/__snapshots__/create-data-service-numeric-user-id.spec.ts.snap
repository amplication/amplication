  id: 42,
  createdIn: new Date(),
  createdAt: new Date(),
  updatedIn: new Date(),
  updatedAt: new Date(),
  email: "exampleEmail",
};

const service = {
  createProfile() {
    return CREATE_RESULT;
  },
  profiles: () => FIND_MANY_RESULT,
