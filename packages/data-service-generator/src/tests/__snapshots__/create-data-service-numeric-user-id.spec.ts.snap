    image: postgres:12
    ports:
      - \${DB_PORT}:5432
    environment:
      POSTGRES_USER: \${DB_USER}
      POSTGRES_PASSWORD: \${DB_PASSWORD}
      POSTGRES_DB: \${DB_NAME}
    volumes:
      - postgres:/var/lib/postgresql/data
    healthcheck:
      test:
        - CMD-SHELL
        - pg_isready -d $\${POSTGRES_DB} -U $\${POSTGRES_USER}
      timeout: 45s
      interval: 10s
      retries: 10
volumes:
  postgres: ~
