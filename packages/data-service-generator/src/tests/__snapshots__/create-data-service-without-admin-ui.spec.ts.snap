      .get(\`\${"/profiles"}/\${existingId}\`)
      .expect(HttpStatus.OK)
      .expect({
        ...FIND_ONE_RESULT,
        createdIn: FIND_ONE_RESULT.createdIn.toISOString(),
        createdAt: FIND_ONE_RESULT.createdAt.toISOString(),
