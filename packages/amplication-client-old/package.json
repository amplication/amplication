{
  "name": "@amplication/client",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@extra-set/difference": "2.2.3",
    "@monaco-editor/react": "^4.4.6",
    "@rooks/use-window-size": "3.6.0",
    "ajv-errors": "3.0.0",
    "axios": "0.27.2",
    "capital-case": "1.0.4",
    "classnames": "2.3.1",
    "date-fns": "2.22.1",
    "deepdash-es": "5.3.9",
    "downloadjs": "1.4.7",
    "events": "3.3.0",
    "formik": "2.2.9",
    "graphql": "15.0.0",
    "lodash": "4.17.21",
    "pluralize": "8.0.0",
    "query-string": "7.1.1",
    "react-beautiful-dnd": "13.1.0",
    "react-command-palette": "^0.21.1",
    "react-compound-timer": "^1.2.0",
    "react-device-detect": "1.17.0",
    "react-diff-viewer": "3.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-draggable": "4.4.3",
    "react-dropzone": "11.3.4",
    "react-helmet": "6.1.0",
    "react-hotkeys": "2.0.0",
    "react-lazylog": "4.5.3",
    "react-query": "3.39.1",
    "react-router-dom": "5.3.3",
    "react-scripts": "4.0.3",
    "react-teleporter": "2.2.1",
    "react-tracking": "8.0.0",
    "react-use-localstorage": "3.5.3",
    "react-xarrows": "1.7.1",
    "sass": "1.35.2",
    "semver": "7.3.5",
    "styled-components": "5.1.1",
    "use-debounce": "3.4.3",
    "xlsx": "0.16.9",
    "yaml": "1.10.2"
  },
  "scripts": {
    "start": "craco start",
    "start:debug": "npm start",
    "build": "CI=false BUILD_PATH=../../dist/apps/@amplication/client craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\"",
    "serve": "npm start"
  },
  "proxy": "http://localhost:3000",
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/self-closing-comp": "error",
      "react/jsx-boolean-value": "error",
      "react/jsx-key": "error",
      "react/jsx-no-useless-fragment": "error",
      "no-duplicate-imports": "error",
      "no-undef": "error",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ]
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!react-command-palette)"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-class-static-block": "^7.14.5",
    "@types/ajv-errors": "1.0.3",
    "@types/amplitude-js": "5.11.3",
    "@types/chalk-pipe": "2.0.0",
    "@types/downloadjs": "1.4.3",
    "@types/lodash": "4.14.182",
    "@types/node": "12.20.16",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.14.11",
    "@types/react-beautiful-dnd": "13.1.1",
    "@types/react-command-palette": "^0.18.0",
    "@types/react-helmet": "6.1.5",
    "@types/react-lazylog": "4.5.1",
    "@types/react-router-dom": "5.3.3",
    "@types/react-tracking": "7.0.4",
    "@types/semver": "7.3.9",
    "jest-mock-extended": "^2.0.4",
    "prettier": "2.1.1"
  }
}
