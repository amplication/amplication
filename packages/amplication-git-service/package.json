{
  "name": "@amplication/git-service",
  "version": "0.13.0",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@apollo/gateway": "0.42.3",
    "@nestjs/common": "8.2.3",
    "@nestjs/config": "1.1.5",
    "@nestjs/core": "8.2.3",
    "@nestjs/graphql": "9.1.2",
    "@nestjs/platform-express": "8.2.3",
    "@octokit/openapi-types": "2.3.1",
    "graphql": "15.7.2",
    "octokit": "^1.7.1",
    "octokit-plugin-create-pull-request": "^3.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/testing": "8.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "eslint": "7.23.0",
    "eslint-import-resolver-lerna": "2.0.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "^27.2.5",
    "jest-mock-extended": "2.0.4",
    "prettier": "1.19.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
