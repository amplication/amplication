datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Version {
  changelog  String?
  createdAt  DateTime  @default(now())
  deletedAt  DateTime?
  deprecated Boolean?
  id         String    @id @default(cuid())
  name       String?
  updatedAt  DateTime  @updatedAt
}
