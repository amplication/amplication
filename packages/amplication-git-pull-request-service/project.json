{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/amplication-git-pull-request-service/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/@amplication/git-pull-request-service",
        "tsConfig": "packages/amplication-git-pull-request-service/tsconfig.lib.json",
        "packageJson": "packages/amplication-git-pull-request-service/package.json",
        "main": "packages/amplication-git-pull-request-service/src/index.ts",
        "assets": ["packages/amplication-git-pull-request-service/*.md"]
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/amplication-git-pull-request-service/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/amplication-git-pull-request-service"],
      "options": {
        "jestConfig": "packages/amplication-git-pull-request-service/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "amplication-nest-logger-module",
    "amplication-kafka",
    "amplication-git-service"
  ]
}
