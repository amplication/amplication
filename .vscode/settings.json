{
  "cSpell.words": [
    "Amplication",
    "Authorizable",
    "Formik",
    "IBLOCK",
    "Queryable",
    "Signup",
    "UNREQUIRED",
    "camelcase",
    "checkmark",
    "deepdash",
    "linkcode",
    "nestjs",
    "rmwc",
    "scrollbar",
    "scrollbars",
    "teleporter",
    "uncontained",
    "unelevated",
    "upsert"
  ],
  "eslint.workingDirectories": [
    {
      "pattern": "./packages/*"
    }
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "terraform.languageServer": {},
  "editor.formatOnSave": true,
  "cSpell.ignoreWords": ["exshql"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[prisma]": {
    "editor.defaultFormatter": null
  },
  "[terraform]": {
    "editor.defaultFormatter": null
  }
}
