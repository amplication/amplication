{
  "implicitDependencies": {
    "package.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
		  "ng-packagr-build",
		  "ng-packagr",
		  "build",
		  "lint",
		  "test",
		  "e2e",
		  "extract-i18n"
		]
      }
    }
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "packages"
  },
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
  }
}
